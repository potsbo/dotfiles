Include ~/.ssh/config.d/*
IdentityFile ~/.ssh/id_ed25519

Host mac-mini
  HostName mac-mini
  User shared

Host raptorlake-windows
  HostName raptorlake-windows

Host raptorlake
  HostName raptorlake
  ForwardAgent yes

Host phoenix
  HostName phoenix
  ForwardAgent yes
  RemoteForward /home/potsbo/.opener.sock /Users/potsbo/.opener.sock

Host github.com
  User git
  Port 22
  HostName github.com
